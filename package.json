    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-astro": "^0.31.1",
    "eslint-plugin-import": "^2.31.0",
    "node-fetch": "^3.3.2",
