    "preview": "NODE_ENV=development astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.astro --fix",
    "check-printful": "node scripts/check-printful-mockups.js",
    "get-printful-ids": "node scripts/get-printful-ids.js",
    "test-download": "node scripts/test-download-mockup.js",
    "setup-env": "node scripts/setup-env.js",
    "move-mockups": "node scripts/move-mockups.js",
    "verify-test-config": "tsx src/scripts/verify-test-config.ts"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.3",
