{
  "name": "verdant-velocity",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development astro dev",
    "start": "NODE_ENV=development astro dev",
    "optimize-images": "node scripts/optimize-images.js",
    "build": "npm run optimize-images && NODE_ENV=production astro build",
    "build-dev": "NODE_ENV=development astro build",
    "build-quick": "NODE_ENV=development ASTRO_DISABLE_PRERENDERING=true astro build",
