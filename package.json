    "@sentry/vite-plugin": "^4.0.1",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.5",
    "astro": "^5.12.9",
    "dotenv": "^16.5.0",
    "libphonenumber-js": "^1.12.10",
    "nanostores": "^1.0.1",
    "printful": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.2",
    "slugify": "^1.6.6",
