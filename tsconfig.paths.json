{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@utils/*": ["src/utils/*"],
      "@types/*": ["src/types/*"],
      "@content/*": ["src/content/*"],
      "@config/*": ["src/config/*"],
      "@scripts/*": ["src/scripts/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@types/printful/*": ["src/types/printful/*"],
      "@types/database/*": ["src/types/database/*"],
      "@types/api/*": ["src/types/api/*"],
      "@types/common/*": ["src/types/common/*"]
    }
  }
} 